<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoAppMovil.Registros">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Registros de Bitacora"
               FontSize="Title"
                HorizontalOptions="CenterAndExpand" />
            <Button x:Name="btnRegistro" Text="Conductor"  FontFamily="{StaticResource FontAwesomeSolid}" CornerRadius="30" FontSize="Title" TextColor="White" BackgroundColor="CadetBlue" Clicked="btnRegistro_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>