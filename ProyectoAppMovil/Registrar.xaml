<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoAppMovil.Registrar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="VENTANA DE REGISTRO" />
            <Entry x:Name="txtNombre" Placeholder="Nombre" />
            <Entry x:Name="txtUsuario" Placeholder="Usuario" />
            <Entry x:Name="txtContraseña" Placeholder="Contraseña"/>
            <Button x:Name="btnGuardar" Text="Guardar"  />
            <Button x:Name="btnSalir" Text="Salir"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>